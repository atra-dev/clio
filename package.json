{
  "name": "clio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "npm run clean:build-artifacts && next dev",
    "dev:webpack": "npm run clean:build-artifacts && next dev --webpack",
    "build": "next build",
    "build:clean": "npm run clean:build-artifacts && next build",
    "start": "next start",
    "lint": "eslint",
    "clean:build-artifacts": "node scripts/clean-build-artifacts.mjs",
    "env:firebase-admin": "node scripts/apply-firebase-admin-env.js",
    "audit": "npm audit --audit-level=high",
    "audit:full": "npm audit --json"
  },
  "dependencies": {
    "firebase": "^12.9.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^10.0.2",
    "eslint-config-next": "^0.2.4",
    "tailwindcss": "^4"
  }
}
